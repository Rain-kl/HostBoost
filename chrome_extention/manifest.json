{
	"name": "HostBoost",
	"description": "Manage host configurations easily",
	"version": "1.0.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"icons": {
		"16": "icons/icon16.svg",
		"48": "icons/icon48.svg",
		"128": "icons/icon128.svg"
	}
}